<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <!--Subway Logo-->
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:src="@drawable/subway_menu_logo"/>

            <!-- Plate Bundle -->
            <ImageButton
                android:id="@+id/subway_subwaySeries"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@null"
                app:srcCompat="@drawable/subway_series_subway"/>

            <ImageButton
                android:id="@+id/subway_classicSandwiches"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@null"
                app:srcCompat="@drawable/classic_sandwiches_subway"/>

            <ImageButton
                android:id="@+id/subway_freshMelts"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@null"
                app:srcCompat="@drawable/fresh_melts_subway"/>

            <ImageButton
                android:id="@+id/subway_noBready"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@null"
                app:srcCompat="@drawable/no_bready_bowls_subway"/>

            <ImageButton
                android:id="@+id/subway_onlineExclusive"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@null"
                app:srcCompat="@drawable/online_exclusive_subway"/>

            <ImageButton
                android:id="@+id/subway_wraps"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@null"
                app:srcCompat="@drawable/wraps_subway"/>

            <ImageButton
                android:id="@+id/subway_breakfast"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@null"
                app:srcCompat="@drawable/breakfast_subway"/>

            <ImageButton
                android:id="@+id/subway_salads"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@null"
                app:srcCompat="@drawable/salads_subway"/>

            <ImageButton
                android:id="@+id/subway_freshFit"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@null"
                app:srcCompat="@drawable/fresh_fit_for_kids_subway"/>

            <!--
            <ImageButton
                android:id="@+id/subway_personalPizza"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@null"
                app:srcCompat="@drawable/personal_pizza_subway"/>

            <ImageButton
                android:id="@+id/subway_sides"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@null"
                app:srcCompat="@drawable/sides_subway"/>

            <ImageButton
                android:id="@+id/subway_deserts"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@null"
                app:srcCompat="@drawable/deserts_subway"/>
            -->

            <ImageButton
                android:id="@+id/subway_drinks"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@null"
                app:srcCompat="@drawable/drinks_subway"/>
        </LinearLayout>
    </ScrollView>
</FrameLayout>